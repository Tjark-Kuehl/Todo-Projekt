{
    "name": "todo_projekt",
    "version": "1.0.0",
    "description": "Ein Schulprojekt um sich besser zu organisieren",
    "main": "index.js",
    "scripts": {
        "start": "npm run server",
        "server": "nodemon index.js --exec babel-node",
        "update": "npm prune && npm-check -u"
    },
    "nodemonConfig": {
        "delay": "50",
        "watch": [
            "*.*"
        ],
        "ext": "js html scss sass json svg png jpg jpeg"
    },
    "devDependencies": {
        "nodemon": "^1.17.3"
    },
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "html-minifier": "^3.5.13",
        "jsonwebtoken": "^8.2.1",
        "node-sass": "^4.8.3",
        "pg": "^7.4.1"
    },
    "keywords": [
        "schule",
        "projekt",
        "todo",
        "node",
        "javascript"
    ],
    "author": "Bennet Russnak, Rami Gomaa, Tjark KÃ¼hl",
    "license": "ISC"
}
