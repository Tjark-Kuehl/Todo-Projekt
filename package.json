{
    "name": "todo_projekt",
    "version": "1.0.0",
    "description": "Ein Schulprojekt um sich besser zu organisieren",
    "main": "index.js",
    "scripts": {
        "start": "npm run server",
        "server": "nodemon index.js --exec babel-node",
        "update": "npm prune && npm-check -u"
    },
    "nodemonConfig": {
        "delay": "50",
        "watch": ["*.*"],
        "ext": "js html scss sass json svg png jpg jpeg"
    },
    "devDependencies": {
        "nodemon": "^1.17.4"
    },
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "cookie": "^0.3.1",
        "cookie-signature": "^1.1.0",
        "html-autoprefixer": "^0.3.8",
        "html-minifier": "^3.5.15",
        "jsonwebtoken": "^8.2.1",
        "node-sass": "^4.9.0",
        "pg": "^7.4.2",
        "uglify-es": "^3.3.9"
    },
    "keywords": ["schule", "projekt", "todo", "node", "javascript"],
    "author": "Bennet Russnak, Rami Gomaa, Tjark KÃ¼hl"
}
