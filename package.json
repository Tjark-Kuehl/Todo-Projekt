{
  "name": "todo_projekt",
  "version": "1.0.0",
  "description": "Ein Schulprojekt um sich besser zu organisieren",
  "main": "index.js",
  "scripts": {
    "start": "npm run server",
    "server": "nodemon index.js --exec babel-node"
  },
  "nodemonConfig": {
    "ignore": [
      "out/"
    ],
    "delay": "50",
    "watch": [
      "*.*"
    ],
    "ext": "js html scss sass json"
  },
  "devDependencies": {
    "nodemon": "^1.15.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "clean-css": "^4.1.9",
    "html-minifier": "^3.5.10",
    "jsonwebtoken": "^8.2.0",
    "node-sass": "^4.7.2",
    "pg": "^7.4.1",
    "uglify-es": "^3.3.9"
  },
  "keywords": [
    "schule",
    "projekt",
    "todo",
    "node",
    "javascript"
  ],
  "author": "Bennet Russnak, Rami Gomaa, Tjark KÃ¼hl",
  "license": "ISC"
}
